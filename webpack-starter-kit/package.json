{
  "name": "webpack-starter-kit",
  "version": "1.0.0",
  "description": "Kit de inicio para trabajar con Webpack y Vanilla JS, Vue.js, React, TypeScript y Sass",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "dev2": "webpack --mode development ./prueba/dev/main.js --output ./prueba/public/script.js",
    "build2": "webpack --mode production ./prueba/dev/main.js --output ./prueba/public/script.js",
    "dev-babel": "webpack --mode development --module-bind js=babel-loader",
    "build-babel": "webpack --mode production --module-bind js=babel-loader"
  },
  "author": "Jonathan MirCha <jonmircha@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.2",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9"
  }
}
